<!DOCTYPE html>
<html lang="en">
  <%- include('partials/head.ejs')%>
  <body>
    <div class="col-xxl-4 col-xl-4 col-lg-6 col-md-8 col-sm-10 col-11">
      <%- include('partials/avatar.ejs')%>
      <div class="w-100">
        <form action="/login" autocomplete="off" method="POST">
          <div class="wow fadeInUp" data-wow-delay="200ms">
            <label for="exampleInputEmail1" class="form-label">Username</label>
            <input
              type="text"
              name="username"
              class="form-control"
              id="exampleInputEmail1"
              aria-describedby="emailHelp"
              placeholder="Type an username here"
            />
          </div>

          <div
            class="btn-group d-flex gap-3 my-3 wow fadeInUp"
            data-wow-delay="300ms"
            role="group"
          >
            <input
              type="radio"
              class="btn-check"
              name="gender"
              id="btnradio1"
              value="male"
            />
            <label class="btn click" for="btnradio1">Male</label>
            <input
              type="radio"
              class="btn-check"
              name="gender"
              id="btnradio2"
              value="female"
            />
            <label class="btn click" for="btnradio2">Female</label>
          </div>
          <button
            type="submit"
            class="btn btn-primary w-100 wow fadeInUp"
            data-wow-delay="400ms"
            onclick="location.href = '../aijul/questions-creator.html';"
          >
            Next
          </button>
        </form>
      </div>
    </div>
    <%- include('partials/offcanvas.ejs')%> <%- include('partials/footer.ejs')%>
